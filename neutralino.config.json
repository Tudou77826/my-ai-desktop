{
  "applicationId": "claude.config.manager",
  "version": "0.1.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/dist/",
  "url": "/index.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "*"
  ],
  "modes": {
    "window": {
      "title": "ClaudeCode Config Manager",
      "width": 1200,
      "height": 800,
      "minWidth": 900,
      "minHeight": 600,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/app.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "claude-config-manager",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/js/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.8.0"
  }
}
